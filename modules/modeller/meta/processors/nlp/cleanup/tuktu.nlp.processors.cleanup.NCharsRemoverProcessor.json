	"parameters": [
		{
			"name": "id",
			"type": "string",
			"required": true
		},
		{
			"name": "result",
			"type": "string",
			"required": true
		},
		{
			"name": "config",
			"type": "object",
			"required": true,
			"parameters": [
				{
					"name": "field",
					"description": "The field to remove N chars from.",
					"type": "string",
					"required": true
				},
				{
					"name": "n",
					"label": "N",
