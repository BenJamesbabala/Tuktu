{
    "generators": [
        {
            "result": "data",
            "config": {
                "credentials": {
                    "consumer_key": "...",
                    "consumer_secret": "...",
                    "access_token": "...",
                    "access_token_secret": "..."
                },
                "filters": {
                    "keywords": [
                        "bigdata"
                    ]
                }
            },
            "name": "tuktu.social.generators.TwitterGenerator",
            "next": [
                "twitterSrcAdder"
            ]
        },
        {
            "result": "data",
            "config": {
                "credentials": {
                    "access_token": "..."
                },
                "filters": {
                    "users": [
                        "obama"
                    ]
                },
                "update_time": 5,
                "interval": {
                    "start": null,
                    "end": null
                }
            },
            "name": "tuktu.social.generators.FacebookGenerator",
            "next": [
                "fbSrcAdder"
            ]
        }
    ],
    "processors": [
        {
            "id": "twitterSrcAdder",
            "result": "source",
            "config": {
                "value": "Twitter"
            },
            "name": "tuktu.processors.FieldConstantAdderProcessor",
            "next": [
                "twitterMessageObtainer"
            ]
        },
        {
            "id": "twitterMessageObtainer",
            "result": "",
            "config": {
                "fields": [
                    {
                        "default": "",
                        "path": [
                            "data",
                            "text"
                        ],
                        "result": "message"
                    }
                ]
            },
            "name": "tuktu.processors.JsonFetcherProcessor",
            "next": [
                "tokenizer"
            ]
        },
        {
            "id": "tokenizer",
            "result": "cleanedMsg",
            "config": {
                "field": "message"
            },
            "name": "tuktu.nlp.processors.TokenizerProcessor",
            "next": [
                "liga"
            ]
        },
        {
            "id": "liga",
            "result": "language",
            "config": {
                "field": "message"
            },
            "name": "tuktu.nlp.processors.LIGAProcessor",
            "next": [
                "langFilter"
            ]
        },
        {
            "id": "langFilter",
            "result": "",
            "config": {
                "expressions": [
                    {
                        "type": "simple",
                        "expression": "${language}=nl_NL"
                    }
                ],
                "batch_min_count": 1
            },
            "name": "tuktu.processors.PacketFilterProcessor",
            "next": [
                "postagger"
            ]
        },
        {
            "id": "postagger",
            "result": "POSTags",
            "config": {
                "language": "${language}",
                "tokens": "cleanedMsg"
            },
            "name": "tuktu.nlp.processors.POSTaggerProcessor",
            "next": [
                "rbempol"
            ]
        },
        {
            "id": "rbempol",
            "result": "polarity",
            "config": {
                "language": "${language}",
                "tokens": "cleanedMsg",
                "pos": "POSTags"
            },
            "name": "tuktu.nlp.processors.RBEMPolarityProcessor",
            "next": [
                "fieldImploder"
            ]
        },
        {
            "id": "fieldImploder",
            "result": "",
            "config": {
                "fields": []
            },
            "name": "tuktu.processors.ImploderProcessor",
            "next": [
                "writer",
                "debug"
            ]
        },
        {
            "id": "writer",
            "result": "",
            "config": {
                "file_name": "data/social_data.csv"
            },
            "name": "tuktu.csv.processors.CSVWriterProcessor",
            "next": []
        },
        {
            "id": "debug",
            "result": "",
            "config": {},
            "name": "tuktu.processors.ConsoleWriterProcessor",
            "next": []
        },
        {
            "id": "fbSrcAdder",
            "result": "source",
            "config": {
                "value": "Faceook"
            },
            "name": "tuktu.processors.FieldConstantAdderProcessor",
            "next": [
                "fbMessageObtainer"
            ]
        },
        {
            "id": "fbMessageObtainer",
            "result": "",
            "config": {
                "fields": [
                    {
                        "default": "",
                        "path": [
                            "data",
                            "message"
                        ],
                        "result": "message"
                    }
                ]
            },
            "name": "tuktu.processors.JsonFetcherProcessor",
            "next": [
                "tokenizer"
            ]
        }
    ]
}