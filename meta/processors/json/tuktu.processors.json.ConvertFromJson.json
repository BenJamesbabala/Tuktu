{
	"name": "Convert from JSON Processor",
	"class": "tuktu.processors.json.ConvertFromJson",
	"description": "Takes a JSON field and converts it into something usable",
	"parameters": [
		{
			"name": "id",
			"type": "string",
			"required": true
		},
		{
			"name": "result",
			"type": "string",
			"required": true
		},
		{
			"name": "config",
			"type": "object",
			"required": true,
			"parameters": [
				{
					"name": "field",
					"type": "string",
					"required": true,
					"description": "The field containing the JSON"
				},
				{
					"name": "overwrite",
					"type": "boolean",
					"required": false
				}
			]
		}
	]
}
