{
	"name": "Flattener Processor",
	"description": "Recursively flattens a map object, appending the keys to the previous keys separated by a given separator.",
	"class": "tuktu.processors.FlattenerProcessor",
	"parameters": [
		{
			"name": "id",
			"type": "string",
			"required": true
		},
		{
			"name": "result",
			"type": "string",
			"required": true
		},
		{
			"name": "config",
			"type": "object",
			"required": true,
			"parameters": [
				{
					"name": "fields",
					"description": "The fields to be flattened.",
					"type": "array",
					"required": true,
					"parameters": [
						{
							"name": "",
							"type": "string",
							"required": true
						}
					]
				},
				{
					"name": "separator",
					"description": "The separator inserted between two iterations.",
					"type": "string",
					"required": true
				}
			]
		}
	]
}